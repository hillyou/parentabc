<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.dialect">org.hibernate.dialect.DerbyDialect</property>
        <property name="hibernate.connection.driver_class">org.apache.derby.jdbc.ClientDriver</property>
        <property name="hibernate.connection.url">jdbc:derby://localhost:1527/parentabc</property>
        <property name="hibernate.connection.username">parentabc</property>
        <property name="hibernate.connection.password">parentabc</property>
        <mapping resource="com/parentabc/entity/Answers.hbm.xml"/>
        <mapping resource="com/parentabc/entity/Memberuser.hbm.xml"/>
        <mapping resource="com/parentabc/entity/Questions.hbm.xml"/>
    </session-factory>
</hibernate-configuration>
